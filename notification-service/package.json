{
    "name": "notification-service",
    "version": "1.0.0",
    "description": "Node Core is a NodeJS framework built by cutting edge design and techniques like clean architecture, domain driven design (DDD), singleton pattern, table inheritance, and more..., it's easy to maintain and expand the system, including the transition from monolithic to microservices.",
    "license": "SEE LICENSE IN LICENSE",
    "readmeFilename": "README.md",
    "scripts": {
        "postinstall": "husky install",
        "generate:apidoc": "grunt exec:generate-api-docs",
        "generate:module": "node ./module-generator/generate.js",
        "generate:usecase": "node ./module-generator/generate-usecase.js",
        "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/config/DbConfig",
        "cache:clear": "npm run typeorm:cli -- cache:clear",
        "migration:generate": "npm run typeorm:cli -- migration:generate -n",
        "migration:create": "npm run typeorm:cli -- migration:create -n",
        "migration:up": "npm run typeorm:cli -- migration:run",
        "migration:down": "npm run typeorm:cli -- migration:revert",
        "lint": "eslint --ext .ts --ext .js ./src",
        "build": "grunt clean && grunt sync && tsc",
        "test": "npm run test:unit && npm run test:report",
        "test:unit": "nyc mocha --exit ./src/**/**/*.spec.ts",
        "test:report": "nyc report --reporter=html --reporter=text",
        "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/app.ts",
        "start": "NODE_PATH=./dist node ./dist/app.js"
    },
    "author": {
        "name": "Felix Lee",
        "email": "felix.le.236@gmail.com"
    },
    "homepage": "https://github.com/felixle236/node-core#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/felixle236/node-core.git"
    },
    "bugs": {
        "url": "https://github.com/felixle236/node-core/issues"
    },
    "keywords": [
        "nodejs",
        "node core",
        "node service",
        "node api",
        "typescript",
        "clean architecture",
        "domain driven design",
        "ddd",
        "repository pattern",
        "singleton pattern"
    ],
    "dependencies": {
        "@google-cloud/logging-winston": "^4.1.1",
        "amqplib": "^0.8.0",
        "axios": "^0.24.0",
        "child-process-promise": "^2.2.1",
        "class-transformer": "^0.5.1",
        "class-validator": "0.13.2",
        "class-validator-jsonschema": "^3.1.0",
        "cloudevents": "^5.2.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "ejs": "^3.1.6",
        "env-var": "^7.1.1",
        "express": "^4.20.0",
        "express-basic-auth": "^1.2.1",
        "express-winston": "^4.2.0",
        "glob": "^7.2.0",
        "helmet": "^4.6.0",
        "husky": "^7.0.4",
        "i18n": "^0.13.3",
        "mailgen": "^2.0.16",
        "mailgun-js": "^0.22.0",
        "node-pushnotifications": "^2.0.0",
        "openapi3-ts": "^2.0.1",
        "pg": "^8.7.1",
        "qs": "^6.10.2",
        "redis": "^3.1.2",
        "reflect-metadata": "^0.1.13",
        "routing-controllers": "^0.9.0",
        "routing-controllers-openapi": "^3.1.0",
        "swagger-ui-express": "^4.3.0",
        "twilio": "^3.72.0",
        "typedi": "^0.10.0",
        "typeorm": "^0.2.41",
        "typeorm-typedi-extensions": "^0.4.1",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/amqplib": "^0.8.2",
        "@types/axios": "^0.14.0",
        "@types/express": "^4.17.13",
        "@types/glob": "^7.2.0",
        "@types/i18n": "^0.13.2",
        "@types/mailgen": "^2.0.5",
        "@types/mailgun-js": "^0.22.12",
        "@types/node": "^17.0.4",
        "@types/node-pushnotifications": "^1.0.4",
        "@types/redis": "^2.8.32",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/twilio": "^3.19.3",
        "@types/validator": "^13.7.0",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.2.0",
        "eslint-plugin-standard": "^5.0.0",
        "grunt": "^1.4.1",
        "grunt-contrib-clean": "^2.0.0",
        "grunt-exec": "^3.0.0",
        "grunt-sync": "^0.8.2",
        "nodemon": "^2.0.15",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4"
    }
}
