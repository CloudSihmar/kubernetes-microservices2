{
    "name": "authentication-service",
    "version": "1.0.0",
    "description": "Node Core is a NodeJS framework built by cutting edge design and techniques like clean architecture, domain driven design (DDD), singleton pattern, table inheritance, and more..., it's easy to maintain and expand the system, including the transition from monolithic to microservices.",
    "license": "SEE LICENSE IN LICENSE",
    "readmeFilename": "README.md",
    "scripts": {
        "postinstall": "husky install",
        "generate:module": "node ./module-generator/generate.js",
        "generate:usecase": "node ./module-generator/generate-usecase.js",
        "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/configs/DbConfig",
        "cache:clear": "npm run typeorm:cli -- cache:clear",
        "migration:generate": "npm run typeorm:cli -- migration:generate -n",
        "migration:up": "npm run typeorm:cli -- migration:run",
        "migration:down": "npm run typeorm:cli -- migration:revert",
        "lint": "eslint --ext .ts --ext .js ./src",
        "build": "grunt clean && grunt sync && tsc",
        "test": "npm run test:unit && npm run test:report",
        "test:unit": "nyc mocha --exit ./src/**/*.spec.ts",
        "test:report": "nyc report --reporter=html --reporter=text",
        "dev": "NODE_ENV=local nodemon --exec ts-node -r tsconfig-paths/register src/app.ts",
        "start": "NODE_ENV=production node ./dist/app.js"
    },
    "author": {
        "name": "Felix Lee",
        "email": "felix.le.236@gmail.com"
    },
    "homepage": "https://github.com/felixle236/node-core#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/felixle236/node-core.git"
    },
    "bugs": {
        "url": "https://github.com/felixle236/node-core/issues"
    },
    "keywords": [
        "nodejs",
        "node core",
        "node service",
        "node api",
        "typescript",
        "clean architecture",
        "domain driven design"
    ],
    "dependencies": {
        "@google-cloud/logging-winston": "^4.1.1",
        "amqplib": "^0.8.0",
        "aws-sdk": "^2.987.0",
        "axios": "^0.21.4",
        "child-process-promise": "^2.2.1",
        "class-transformer": "^0.4.0",
        "class-validator": "0.13.1",
        "class-validator-jsonschema": "^3.1.0",
        "cloudevents": "^4.0.3",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-winston": "^4.2.0",
        "jsonwebtoken": "^8.5.1",
        "locale-code": "^2.0.2",
        "module-alias": "^2.2.2",
        "openapi3-ts": "^2.0.1",
        "pg": "^8.7.1",
        "redis": "^3.1.2",
        "reflect-metadata": "^0.1.13",
        "routing-controllers": "^0.9.0",
        "routing-controllers-openapi": "^3.1.0",
        "swagger-ui-express": "^4.1.6",
        "typedi": "^0.8.0",
        "typeorm": "^0.2.37",
        "typeorm-typedi-extensions": "^0.4.1",
        "uuid": "^8.3.2",
        "winston": "^3.3.3",
        "winston-cloudwatch": "^3.0.2"
    },
    "devDependencies": {
        "@types/amqplib": "^0.8.2",
        "@types/axios": "^0.14.0",
        "@types/chai": "^4.2.21",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/mocha": "^9.0.0",
        "@types/module-alias": "^2.0.1",
        "@types/node": "^16.9.1",
        "@types/redis": "^2.8.32",
        "@types/redis-mock": "^0.17.0",
        "@types/sinon": "^10.0.2",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^8.3.1",
        "@types/validator": "^13.6.3",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^4.31.0",
        "@typescript-eslint/parser": "^4.31.0",
        "chai": "^4.3.4",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-standard": "^5.0.0",
        "grunt": "^1.4.1",
        "grunt-contrib-clean": "^2.0.0",
        "grunt-exec": "^3.0.0",
        "grunt-sync": "^0.8.2",
        "husky": "^7.0.2",
        "mocha": "^9.1.1",
        "nodemon": "^2.0.12",
        "nyc": "^15.1.0",
        "redis-mock": "^0.56.3",
        "sinon": "^11.1.2",
        "ts-node": "^10.2.1",
        "tsconfig-paths": "^3.11.0",
        "typescript": "^4.4.3"
    }
}
